{
    "name": "kozsuper/table-views",
    "description": "A simple table views package for Laravel",
    "version": "1.0.0",
    "authors": [
        {
            "name": "KozSuper",
            "email": "kozsuper@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.9",
        "guava/filament-icon-picker": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KozSuper\\TableViews\\Providers\\TableViewsServiceProvider"
            ],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "KozSuper\\TableViews\\": "src/",
            "KozSuper\\TableViews\\Database\\Factories\\": "database/factories/",
            "KozSuper\\TableViews\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KozSuper\\TableViews\\Tests\\": "tests/"
        }
    }
}